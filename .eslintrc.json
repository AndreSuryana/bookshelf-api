{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "google",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": "off",
        "indent": ["error", 4],
        "eol-last": "off",
        "object-curly-spacing": "off",
        "padded-blocks": "off",
        "max-len": "off"
    }
}
